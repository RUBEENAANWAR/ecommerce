
<section>
    <div class="container">
      <table class="table mt-2">
      <div class="row">
        <a href="/admin/add-product" class="btn btn-success m-3 ml-auto">ADD PRODUCT</a>
      </div>
<table class="table table-hover">  
  <table class="table">
  <thead>
    <tr>
      <th scope="col">NO.</th>
      <th scope="col">Title</th>
      <th scope="col">Category</th>
      <th scope="col">Description</th>
      <th>image</th> 
      <th scope="col">Price</th>
    </tr>
  </thead>
  <tbody>
    {{#each products }}
    <tr>
      
      <th scope="row">1</th>
      <td>{{this.Name}}</td>
      <td>{{this.Category}}</td>
      <td>{{this.Description}}</td>
      <td><img style="width:50px; height:50px" src="/product-images/{{this._id}}.jpg" alt="baby dresses"></td>
    </tr>
  {{/each}}
  </tbody>
</table>
</table>
</table>
</div>
</div>
</section>